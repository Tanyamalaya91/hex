{
  "Name": "jane",
  "LogFile": "/var/log/jane.log",
  "Connectors": [

    {"Type": "slack", "ID": "slack", "Active": true,
      "Key": "<SlackToken>", "Image": ":game_die:"
    },

    {"Type": "rss", "ID": "Bamboo Build", "Active": true,
      "Server": "https://BambooUser:BambooPass@somecompany.atlassian.net/builds/plugins/servlet/streams?local=true",
      "SuccessMatch": "successful", "FailureMatch": "fail",
      "Routes": [
        {"Match": "*", "Connectors": "slack", "Target": "#devops"},
        {"Match": "NextGen", "Connectors": "slack", "Target": "#nextgen"}
      ]
    },

    {"Type": "rss", "ID": "Atlassian Status", "Active": true,
      "Server": "http://status.atlassian.com/history.rss",
      "Routes": [
        {"Match": "*", "Connectors": "slack", "Target": "#devops"}
      ]
    },

    {"Type": "rss", "ID": "AWS EC2", "Active": true,
      "Server": "http://status.aws.amazon.com/rss/ec2-us-east-1.rss",
      "Routes": [
        {"Match": "*", "Connectors": "slack", "Target": "#devops"}
      ]
    },

    {"Type": "monitor", "ID": "Elasticsearch Node", "Active": true,
      "Server": "elasticsearch1.somecompany.com", "Login": "jane", "Pass": "abc123",
      "SuccessMatch": "OK", "WarningMatch": "WARNING", "FailureMatch": "CRITICAL",
      "Checks": [
        {"Name": "Apt Check", "Check": "/usr/lib/nagios/plugins/check_apt"},
        {"Name": "Disk Check", "Check": "/usr/lib/nagios/plugins/check_disk -w10% -c5% -A"},
        {"Name": "Elasticsearch Check", "Check": "/usr/lib/nagios/plugins/check_procs -a elasticsearch -c1:1"}
      ],
      "Routes": [
        {"Match": "*", "Connectors": "slack", "Target": "#devops"},
        {"Match": "*", "Connectors": "slack", "Target": "@matt"}
      ]
    },

    {"Type": "website", "ID": "Website Monitor", "Active": true,
      "Checks": [
        {"Name": "Yahoo", "Check": "https://www.yahoo.com"},
        {"Name": "Google", "Check": "https://google.com"}
      ],
      "Routes": [
        {"Match": "*", "Connectors": "slack", "Target": "#devops"},
        {"Match": "*", "Connectors": "slack", "Target": "@matt"}
      ]
    },

    {"Type": "jira", "ID": "jira", "Active": true, "Debug": true,
       "Server": "<URL>.atlassian.net", "Login": "<JIRA USER>", "Pass": "<JIRA PASS>"
    },

    {"Type": "wolfram", "ID": "wolf", "Active": true, "Debug": true,
      "Key": "<WOLFRAM API KEY>"
    },

    {"Type": "imageme", "ID": "imageme", "Active": true,
      "Key": "<GOOGLE API>", "Pass": "<GOOGLE CX ??? >"
    },

    {"Type": "response", "ID": "Text Response", "Active": true, "Debug": true,
        "Commands": [
            {"Match": "jane", "Output": "\"But I have eyes,\" she said. \"And ears. I see everything in all the Hundred Worlds. I watch the sky through a thousand telescopes. I overhear a trillion conversations every day.\" She giggled a little. \"I'm the best gossip in the universe.\""},
            {"Match": "jane", "Output": "\"Twisted and perverse are the ways of the human mind,\" Jane intoned. \"Pinocchio was such a dolt to try to become a real boy. He was much better off with a wooden head.\""},
            {"Match": "jane", "Output": "\"It's the most charming thing about humans. You are all so sure that the lesser animals are bleeding with envy because they didn't have the good fortune to be born Homo sapiens.\""}
        ]
    },

    {"Type": "exec", "ID": "Exec", "Active": true,
        "Commands": [
            {"Match": "big", "Output": "```%stdout%```", "Cmd": "/usr/bin/figlet", "Args": "-w80 %msg%"},
            {"Match": "log", "Output": "```%stdout% ```", "Cmd": "/usr/bin/tail", "Args": "-n8 /var/log/jane.log"},
            {"Match": "cal", "Output": "```%stdout% ```", "Cmd": "/usr/bin/cal", "Args": "-h %msg%"}
        ]
    }

  ]
}
